{
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint", "prettier"],
  "rules": {
    // "@typescript-eslint/no-explicit-any": 2,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "all",
        "tabWidth": 2,
        "printWidth": 120,
        "singleQuote": true,
        "semi": false,
        "arrowParens": "always",
        "bracketSpacing": true,
        "jsxSingleQuote": false
      }
    ]
  }
}
